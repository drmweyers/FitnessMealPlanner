{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "vitest run",
    "test:gui": "vitest run --config=test/gui/vitest.gui.config.ts",
    "test:gui:watch": "vitest --config=test/gui/vitest.gui.config.ts",
    "test:gui:runner": "tsx test/gui/test-runner.ts",
    "test:invitation": "tsx test/gui/run-invitation-tests.ts",
    "test:invitation:simple": "node test-invitation-feature.js",
    "test:trainer-meal-plans": "tsx test/integration/run-trainer-meal-plan-tests.ts",
    "test:integration": "vitest run test/integration/**/*.test.ts",
    "test:unit": "tsx test/unit/testRunner.ts",
    "test:unit:watch": "vitest test/unit/**/*.test.ts",
    "test:unit:coverage": "vitest run test/unit/**/*.test.ts --coverage",
    "test:email": "vitest run test/unit/emailService.test.ts test/email-service.test.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:gui",
    "test:baseline": "TEST_MODE=baseline npm run test:gui",
    "test:playwright": "playwright test",
    "test:playwright:headed": "playwright test --headed",
    "test:business-logic": "playwright test test/e2e/business-logic-visual.spec.ts --headed",
    "test:business-logic:debug": "playwright test test/e2e/business-logic-visual.spec.ts --headed --debug",
    "test:business-logic:robust": "playwright test test/e2e/business-logic-robust.spec.ts --headed",
    "test:business-logic:simple": "playwright test test/e2e/business-logic-simple-visual.spec.ts --headed",
    "test:comprehensive": "tsx test/test-runner.ts",
    "seed:test-accounts": "tsx server/db/seeds/test-accounts.ts",
    "test:comprehensive:unit": "tsx test/test-runner.ts --unit-only",
    "test:comprehensive:critical": "tsx test/test-runner.ts --skip-optional",
    "test:comprehensive:fast": "tsx test/test-runner.ts --skip-optional --fail-fast",
    "test:components": "vitest run test/unit/components/**/*.test.{ts,tsx} --reporter=verbose",
    "test:api": "vitest run test/unit/api/**/*.test.ts --reporter=verbose",
    "test:database": "vitest run test/unit/database/**/*.test.ts --reporter=verbose",
    "test:auth": "vitest run test/unit/auth/**/*.test.ts --reporter=verbose",
    "test:workflows": "vitest run test/integration/**/*.test.ts --reporter=verbose",
    "test:coverage:full": "vitest run --coverage --reporter=verbose --coverage.reporter=text --coverage.reporter=html --coverage.reporter=json",
    "test:trainer-customer": "tsx test/unit/runTrainerCustomerTests.ts",
    "test:trainer-customer:quick": "vitest run test/unit/trainerCustomerRelationships.test.ts test/unit/customerInvitationSystem.test.ts --reporter=basic",
    "qa:api": "node test-api-comprehensive.js",
    "qa:database": "node test-database-integrity.js",
    "qa:e2e": "playwright test test/e2e/qa-comprehensive-e2e.spec.ts",
    "qa:comprehensive": "node run-comprehensive-qa-tests.js",
    "qa:full-suite": "node run-full-qa-suite.js",
    "test:redis": "tsx test/redis/redis-test-runner.ts",
    "test:redis:unit": "tsx test/redis/redis-test-runner.ts --unit",
    "test:redis:integration": "tsx test/redis/redis-test-runner.ts --integration",
    "test:redis:performance": "tsx test/redis/redis-test-runner.ts --performance",
    "test:redis:chaos": "tsx test/redis/redis-test-runner.ts --chaos",
    "test:redis:load": "tsx test/redis/redis-test-runner.ts --load",
    "test:redis:benchmark": "tsx test/redis/redis-test-runner.ts --benchmark",
    "test:redis:production-ready": "tsx test/redis/redis-test-runner.ts --unit --integration --performance --benchmark --production",
    "redis:setup": "docker-compose -f test/redis/setup/docker-compose.redis-test.yml up -d",
    "redis:teardown": "docker-compose -f test/redis/setup/docker-compose.redis-test.yml down",
    "test:enhanced": "tsx test/infrastructure/test-runner-enhanced.ts",
    "test:enhanced:unit": "tsx test/infrastructure/test-runner-enhanced.ts --categories=unit",
    "test:enhanced:integration": "tsx test/infrastructure/test-runner-enhanced.ts --categories=integration",
    "test:enhanced:e2e": "tsx test/infrastructure/test-runner-enhanced.ts --categories=e2e",
    "test:enhanced:performance": "tsx test/infrastructure/test-runner-enhanced.ts --categories=performance",
    "test:enhanced:parallel": "tsx test/infrastructure/test-runner-enhanced.ts --parallel --retry",
    "test:enhanced:coverage": "tsx test/infrastructure/test-runner-enhanced.ts --coverage --output=test-results.json",
    "test:enhanced:fast": "tsx test/infrastructure/test-runner-enhanced.ts --categories=unit --parallel --fail-fast",
    "test:enhanced:critical": "tsx test/infrastructure/test-runner-enhanced.ts --suites=unit-api,integration-workflows,e2e-critical-workflows",
    "test:optimized": "vitest run --config=test/vitest-config-optimized.ts",
    "test:optimized:watch": "vitest --config=test/vitest-config-optimized.ts",
    "test:optimized:coverage": "vitest run --config=test/vitest-config-optimized.ts --coverage",
    "test:customer-visibility": "vitest run test/unit/api/trainer-routes-comprehensive.test.ts --config=test/vitest-config-optimized.ts",
    "qa:automated": "tsx test/automation/qa-automation.ts",
    "qa:security": "tsx test/automation/qa-automation.ts | grep -E '(Security|Customer Data|Authentication|Input Validation|Sensitive Data)'",
    "qa:performance": "tsx test/automation/qa-automation.ts | grep -E '(Performance|Bundle Size|Database Query)'",
    "qa:coverage": "tsx test/automation/qa-automation.ts | grep -E '(Coverage|Unit Test|API Endpoint)'",
    "generate:test-data": "tsx scripts/generate-test-data.js",
    "enhance:test-data": "tsx scripts/enhance-test-data.js",
    "update:test-accounts": "tsx scripts/update-test-accounts.js",
    "check": "tsc",
    "db:generate": "drizzle-kit generate",
    "migrate": "tsx scripts/runMigration.ts",
    "db:push": "drizzle-kit push",
    "create-admin": "tsx scripts/createFirstAdmin.ts",
    "docker:dev": "docker-compose --profile dev up -d",
    "docker:dev:logs": "docker logs fitnessmealplanner-dev -f",
    "docker:dev:stop": "docker-compose --profile dev down",
    "docker:dev:restart": "docker-compose --profile dev restart",
    "docker:dev:rebuild": "docker-compose --profile dev up -d --build",
    "docker:prod": "docker-compose --profile prod up -d",
    "docker:prod:stop": "docker-compose --profile prod down",
    "setup:check": "node scripts/check-setup.js",
    "postinstall": "node scripts/check-setup.js || true"
  },
